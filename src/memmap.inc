# 这是注释的格式，开头是 # 号，如果直接是ctrl + / 默认是 ; 这样是不对的
.equ GPIO_CTRL_ADDR,0x10012000

.equ MTIME,0x0200BFF8
# 32768Hz  约等于 33KHz 后续提及
# 1s = 1000ms  在1ms 里面 MTIME的变化大概就是加了33.
# 200*33+MTIME（原来的数值） = 延时结束后的数值。
.equ FHZ,33
.equ MTIMECOMPARE,0x02004000
